#include<stdio.h>

int chiahet(int n, int k)
{
	     int i;
	     int count = 0;
	     long long mu = 1;
	     long long tich = 1;
	     for(i = 1;i<=n;i++)
	        {
		        tich*=i;
	        }
	     while(count < k)
	        {
		        mu*=2;
		        count++;
        	}
	     if(tich % mu == 0)
	        {
        		return 1;
	        }
	  return 0;
}
int main()
{   
    int n,k;
    scanf("%d %d",&n,&k);
    if(chiahet(n,k) == 1)
    {
    	printf("Yes\n");
	  }
	else
	{
		printf("No\n");
	}
	return 0;
}
